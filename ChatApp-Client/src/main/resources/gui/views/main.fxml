<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainController">
   <children>
      <ListView fx:id="usersList" prefHeight="400.0" prefWidth="151.0" style="-fx-background-color: SKYBLUE; -fx-control-inner-background: SKYBLUE;" />
      <ListView fx:id="messageView" layoutX="151.0" prefHeight="373.0" prefWidth="449.0" />
      <TextField fx:id="name" layoutX="151.0" layoutY="373.0" prefHeight="25.0" prefWidth="149.0" />
      <TextField fx:id="message" layoutX="301.0" layoutY="373.0" prefHeight="25.0" prefWidth="222.0" />
      <Button fx:id="send" layoutX="523.0" layoutY="373.0" mnemonicParsing="false" prefHeight="19.0" prefWidth="77.0"
              style="-fx-background-color: ROYALBLUE;" text="Send" textFill="WHITE"
      onMouseClicked="#onSendButtonPressed"/>
   </children>
</AnchorPane>
